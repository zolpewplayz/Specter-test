FEBRIGHT.BackgroundColor3 = Color3.fromRGB(6, 43, 59)


FEBRIGHT.BorderColor3 = Color3.fromRGB(18, 57, 229)


FEBRIGHT.Position = UDim2.new(0.390532553, 0, 0.300387591, 0)


